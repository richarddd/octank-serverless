{
  "name": "infrastructure",
  "version": "0.1.0",
  "bin": {
    "infrastructure": "src/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "predeploy": "yarn build && cdk bootstrap --profile octank",
    "deploy:fe": "node src/deploy-frontend.js octank",
    "deploy": "yarn cdk deploy --all --require-approval never --profile octank --outputs-file outputs.json && yarn deploy:fe",
    "destroy": "yarn cdk destroy --all --require-approval never --profile octank"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.103.0",
    "@types/jest": "26.0.23",
    "@types/mime-types": "2.1.0",
    "@types/node": "15.0.2",
    "aws-cdk": "1.103.0",
    "esbuild": "0.11.20",
    "jest": "26.6.3",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2": "1.103.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "1.103.0",
    "@aws-cdk/aws-cloudfront": "1.103.0",
    "@aws-cdk/aws-cognito": "1.103.0",
    "@aws-cdk/aws-ec2": "1.103.0",
    "@aws-cdk/aws-lambda-nodejs": "1.103.0",
    "@aws-cdk/aws-rds": "1.103.0",
    "@aws-cdk/aws-s3-deployment": "1.103.0",
    "@aws-cdk/aws-secretsmanager": "1.103.0",
    "@aws-cdk/aws-ssm": "1.103.0",
    "@aws-cdk/core": "1.103.0",
    "aws-sdk": "2.904.0",
    "mime-types": "2.1.30",
    "source-map-support": "0.5.19"
  }
}
